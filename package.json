{
  "name": "react-formik-ui",
  "author": "Kai Hotz",
  "version": "0.1.7",
  "description": "Plain form components for easy use with formik",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build": "node scripts/build.js",
    "test": "jest",
    "test:coverage": "jest --coverage --forceExit --colors",
    "lint": "esw --ext .jsx --ext .js --color",
    "lint:fix": "eslint src -- --fix",
    "prerelease": "npm run lint && npm run test:coverage && npm run build",
    "release": "npm run prerelease && npm publish"
  },
  "engines": {
    "node": ">=8.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KaiHotz/react-formik-ui.git"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.22.1",
    "react-click-outside": "^3.0.1",
    "react-datepicker": "^1.4.1"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "^0.28.11",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-watch": "^3.1.5",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "formik": "^0.11.11",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jasmine-expect": "^3.8.3",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "jsdom": "11.10.0",
    "jsdom-global": "3.0.2",
    "node-sass": "^4.8.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.4.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-styleguidist": "^7.0.14",
    "resolve": "1.6.0",
    "sass-loader": "^7.0.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "yup": "^0.25.1"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x",
    "formik": ">=0.11.11"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
